{
  "expo": {
    "name": "DentalFlow",
    "slug": "DentalFlow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./dental-flow-app-icon.jpg",
    "scheme": "dentalflow",
    "linking": {
      "prefixes": ["dentalflow://"]
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.DentalFlow",
      "infoPlist": {
        "NSCameraUsageDescription": "DentalFlow needs camera access to scan barcodes for inventory management.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./dental-flow-app-icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.anonymous.DentalFlow"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "29486aca-25bb-4a27-ada4-84f5df37c894"
      }
    }
  }
}
