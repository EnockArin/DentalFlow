{
  "expo": {
    "name": "DentalFlow",
    "slug": "DentalFlow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app-icon.png",
    "scheme": "dentalflow",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.DentalFlow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.anonymous.DentalFlow"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow DentalFlow to access camera for barcode scanning."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "29486aca-25bb-4a27-ada4-84f5df37c894"
      }
    }
  }
}
